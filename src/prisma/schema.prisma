generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hello {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar
  createdAt DateTime @db.Timestamp(6) @default(now())
  updatedAt DateTime @db.Timestamp(6) @updatedAt
}
