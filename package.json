{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Node.js template created using express and prisma",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/dasolari/express-template",
  "author": "Diego Solari <dasolari@uc.cl>",
  "license": "MIT",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node .",
    "dev": "nodemon --experimental-specifier-resolution=node .",
    "lint": "eslint ./src index.js",
    "lint-fix": "eslint --fix ./src index.js",
    "prisma:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:sync:force": "npx prisma db push"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "node src/prisma/seeds"
  },
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.1",
    "express": "^4.17.2",
    "express-ejs-layouts": "^2.5.1",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "webpack": "^5.65.0",
    "webpack-dev-middleware": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "ejs": "^3.1.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "file-loader": "^6.2.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.4.6",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.7.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack-command": "^0.5.1",
    "webpack-manifest-plugin": "^4.1.1"
  },
  "engines": {
    "node": ">=16.13.0",
    "yarn": "^1.22.0"
  }
}
